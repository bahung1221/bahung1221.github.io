---
layout: none
---

[
  {% for post in site.external_posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ post.original_url }}",
      "date"     : "{{ post.crawl_date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]